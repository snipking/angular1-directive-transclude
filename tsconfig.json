{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "rootDir": "./src",
        "outDir": "./www"
    },
    "exclude": [
        "node_modules",
        "src/bower_components",
        "www"
    ]
}
